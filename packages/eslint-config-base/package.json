{
  "name": "@heinzelman-labs/eslint-config-base",
  "version": "1.0.0",
  "description": "Heinzelman basic ESLint configuration based on eslint-config-airbnb-base",
  "main": "index.js",
  "repository": "git@gitlab.com:fluffy-heinzelman/eslint-configs.git",
  "homepage": "https://gitlab.com/fluffy-heinzelman/eslint-configs",
  "bugs": {
    "url": "https://gitlab.com/fluffy-heinzelman/eslint-configs/-/issues"
  },
  "author": "Micha Meyer <heinzelman.code@gmail.com>",
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintconfig",
    "config",
